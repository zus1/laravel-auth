{
    "name": "zus1/laravel-auth",
    "description": "Laravel authentication and authorization for laravel",
    "keywords": ["laravel", "authentication", "auth", "authorization", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Ožuška",
            "email": "mario.ozuska@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/mail": "^11.0",
        "illuminate/pagination": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zus1\\LaravelAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zus1\\LaravelAuth\\Providers\\LaravelAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
